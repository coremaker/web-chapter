{
    "name": "@web-chapter/ui",
    "version": "0.0.40",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "type": "module",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
